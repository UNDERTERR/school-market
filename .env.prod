# 生产环境配置 - 强密码（需要设置）
# 复制为 .env.prod 并设置真实密码

# 数据库配置
MYSQL_ROOT_PASSWORD=CHANGE_ME_STRONG_PASSWORD
MYSQL_USER=prod_user
MYSQL_PASSWORD=CHANGE_ME_STRONG_PASSWORD
MYSQL_PORT=3306

# Redis配置
REDIS_PORT=6379
REDIS_PASSWORD=CHANGE_ME_REDIS_PASSWORD

# RabbitMQ配置
RABBITMQ_USER=prod_user
RABBITMQ_PASSWORD=CHANGE_ME_RABBITMQ_PASSWORD
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

# Elasticsearch配置
ELASTICSEARCH_PORT=9200
ELASTIC_PASSWORD=CHANGE_ME_ELASTIC_PASSWORD

# Nacos配置
NACOS_PORT=8848
NACOS_AUTH_ENABLE=true
NACOS_AUTH_TOKEN=CHANGE_ME_32_CHARACTER_NACOS_TOKEN

# 应用配置
GATEWAY_PORT=8080

# 安全配置
SECRET_KEY=CHANGE_ME_JWT_SECRET_KEY_32_CHARS